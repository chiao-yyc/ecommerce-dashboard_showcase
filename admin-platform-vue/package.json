{
  "name": "admin-platform-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier . --write",
    "docs:generate": "typedoc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest tests/unit",
    "test:unit:run": "vitest run tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage",
    "test:unit:html": "vitest run tests/unit --reporter=default --reporter=html",
    "test:integration": "vitest tests/integration",
    "test:integration:run": "vitest run tests/integration",
    "test:integration:coverage": "vitest run tests/integration --coverage",
    "test:quick": "bash scripts/test-groups.sh quick",
    "test:full": "bash scripts/test-groups.sh full",
    "test:groups": "bash scripts/test-groups.sh",
    "test:ai": "vitest run tests/unit/api/services/ai tests/unit/composables/useAI* --reporter=verbose",
    "test:smoke": "vitest run --reporter=default --bail=1",
    "test:coverage:detailed": "vitest run --coverage --reporter=verbose",
    "test:coverage:ci": "vitest run --coverage --reporter=json",
    "test:performance": "vitest run tests/performance --reporter=verbose",
    "test:views": "vitest run tests/unit/views --reporter=verbose",
    "test:charts": "vitest run tests/unit/components/charts --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "typecheck:diagnose": "vue-tsc -p tsconfig.build.json --diagnostics",
    "type-build": "vue-tsc -p tsconfig.build.json",
    "precommit-check": "npm run format && npm run lint && npm run test:run",
    "update-docs": "node ../tools/generate-component-docs.js",
    "check-docs": "node ../tools/check-docs-sync.js",
    "setup-hooks": "bash ../tools/setup-git-hooks.sh",
    "cleanup-logs": "bash ../tools/cleanup-sync-logs.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/vue-query": "^5.74.6",
    "@tanstack/vue-table": "^8.21.3",
    "@types/jspdf": "^1.3.3",
    "@types/marked": "^5.0.2",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vee-validate/zod": "^4.15.0",
    "@vueuse/core": "^13.5.0",
    "autoprefixer": "^10.4.21",
    "canvg": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.503.0",
    "marked": "^16.2.1",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "postcss": "^8.5.3",
    "reka-ui": "^2.3.2",
    "svg2pdf.js": "^2.5.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.1",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.1",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/papaparse": "^5.3.16",
    "@types/vue-virtual-scroller": "npm:@earltp/vue-virtual-scroller",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vitest/coverage-istanbul": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.6",
    "playwright": "^1.54.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.91.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.1",
    "vitest": "^3.1.2",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  }
}